#include <iostream>

using namespace std;
int main()
{
	int N;
	for (int i = 0; i < 3; i++)
	{
		int temp = 0;
		for (int j = 0; j < 4; j++)
		{
			cin >> N;
			temp += N;
		}
		if (temp == 4)
			cout << "E" << '\n';
		else if (temp == 3)
			cout << "A" << '\n';
		else if (temp == 2)
			cout << "B" << '\n';
		else if (temp == 1)
			cout << "C" << '\n';
		else if (temp == 0)
			cout << "D" << '\n';

		temp = 0;
	}
}
